{
  "name": "class-search",
  "private": true,
  "version": "0.1.2",
  "type": "module",
  "main": "dist-electron/electron.js",
  "scripts": {
    "prebuildModel": "tsx model.ts",
    "dev": "npx tsc -p tsconfig.electron.json && concurrently \"vite\" \"wait-on http://localhost:5173 && electron dist-electron/electron.js\"",
    "build": "tsx model.ts && vite build && tsc -p tsconfig.electron.json && electron-builder",
    "testBuild": "tsx model.ts && vite build && tsc -p tsconfig.electron.json && electron-builder --publish never",
    "lint": "eslint .",
    "preview": "vite preview",
    "prettier": "prettier --write .",
    "prettier-check": "prettier --check .",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.25.0",
    "@emotion/react": "^11.14.0",
    "@huggingface/transformers": "^3.7.3",
    "@iconify/react": "^6.0.1",
    "@lancedb/lancedb": "^0.22.0",
    "@tanstack/react-virtual": "^3.13.12",
    "add": "^2.0.6",
    "apache-arrow": "^18.1.0",
    "dayjs": "^1.11.18",
    "idb": "^8.0.3",
    "lucide-react": "^0.542.0",
    "markdown-it-footnote": "^4.0.0",
    "next-themes": "^0.4.6",
    "node-fetch-cache": "^5.0.2",
    "parse-full-name": "^1.2.6",
    "prettier": "^3.6.2",
    "rate-my-professor-api-ts": "^1.0.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.2",
    "react-scan": "^0.4.3",
    "react-spinners": "^0.17.0",
    "react-virtuoso": "^4.14.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/electron": "^1.4.38",
    "@types/jquery": "^3.5.33",
    "@types/markdown-it-footnote": "^3.0.4",
    "@types/node": "^24.3.1",
    "@types/parse-full-name": "^1.2.5",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "concurrently": "^9.2.1",
    "electron": "^37.4.0",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.18.0",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^15.14.0",
    "process": "^0.11.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.11",
    "vite-plugin-pwa": "^0.21.1",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitepress": "^2.0.0-alpha.12",
    "wait-on": "^8.0.4",
    "workbox-core": "^7.3.0",
    "workbox-window": "^7.3.0"
  },
  "overrides": {
    "sharp": "0.32.6",
    "sharp-ico": "0.1.5"
  },
  "build": {
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "appId": "com.brian-kwong.csu-class-search",
    "productName": "CSU Class Search",
    "directories": {
      "buildResources": "buildResources"
    },
    "icon": "buildResources/icon.png",
    "extraResources": [
      "data/local-db/**/*"
    ],
    "asarUnpack": [
      "data/local-db/**/*"
    ],
    "linux": {
      "target": [
        "AppImage"
      ],
      "publish": [
        "github"
      ]
    },
    "snap": {
      "publish": "never"
    }
  }
}
